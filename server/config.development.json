{
  "amino3Config": {
    "noListen": false,
    "noServices": false,
    "logLevel": "info",
    "logToFile": true,
    "logFileFolder": "/tmp/amino3-logs",
    "suppressedServices": [
      "_Authentication",
      "FileUpload",
      "FolderMonitor",
      "_InitializeDatabase",
      "PluginManager",
      "_RebuildClient",
      "RemoteLogging",
      "StaticService",
      "ServerServicesManager",
      "_WebSocketManager"
    ],
    "postalPublishToClientTopicSuppressList": [
      "ServerHeartbeat"
    ],
    "loggerCallerFilenamesToIgnore": [
      "_postal-socket-connection-impl",
      "globals",
      "initialize-database",
      "service-manager",
      "logger",
      "boot-manager"
    ]
  }
}
