{
  "amino3Config": {
    "noListen": false,
    "noServices": false,
    "logLevel": "info",
    "logToFile": true,
    "logFileFolder": "/tmp/amino3-logs",
    "suppressedServices": [
      "_Authentication",
      "FileUpload",
      "FolderMonitor",
      "_InitializeDatabase",
      "PluginManager",
      "RebuildClient",
      "RemoteLogging",
      "StaticService",
      "ServerServicesManager",
      "WebSocketManager"
    ],
    "loggerCallerFilenamesToIgnore": [
      "postal-socket-connection-impl",
      "_globals",
      "_service-manager"
    ]
  }
}
