{
  "name": "AminoUser",
  "plural": "AminoUsers",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "excludeBaseProperties": [
    "emailVerified",
    "realm",
    "id"
  ],
  "properties": {
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "AminoAccessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    },
    "roles": {
      "type": "hasMany",
      "model": "AminoRole",
      "foreignKey": "principalId",
      "keyThrough": "roleId",
      "through": "AminoRoleMapping"
    }
  },
  "acls": [],
  "methods": {}
}
